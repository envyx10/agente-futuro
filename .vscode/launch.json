{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Container reference extractor",
      "type": "debugpy",
      "request": "launch",
      "program": "/workspaces/ai-solver/src/01_container_ref_extractor/main.py",
      "args": ["--images", "/workspaces/ai-solver/data/images/containers"],
      "envFile": "/workspaces/ai-solver/src/01_container_ref_extractor/.env",
      "console": "integratedTerminal"
    },
    {
      "name": "FastMCP Random Tool",
      "type": "debugpy",
      "request": "launch",
      "program": "/workspaces/ai-solver/src/examples/fastmcp_random.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "/workspaces/ai-solver/src"
      }
    },
    {
      "name": "MCPO Proxy (FastMCP Random)",
      "type": "debugpy",
      "request": "launch",
      "module": "mcpo",
      "args": [
        "--port",
        "8000",
        "--api-key",
        "dev-secret",
        "--",
        "python3",
        "src/examples/fastmcp_random.py"
      ],
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "/workspaces/ai-solver/src"
      }
    }
  ]
}
